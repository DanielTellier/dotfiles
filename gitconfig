[user]
  email = telldanieljames@gmail.com
  name = DanielTellier
[core]
  editor = nvim
[alias]
  ac = !git add -A && git commit -m
  acp = "!f() { git add --all && git commit -m \"$1\" && git push; }; f"
[filter "lfs"]
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
  clean = git-lfs clean -- %f
